{
  "name": "discord-bot-server",
  "version": "0.1.1",
  "main": "src/index.ts",
  "repository": "git@github.com:mpnproject/discord-bot-server.git",
  "author": "Gabriel Anatrone <anatrone.gabriel@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.17",
    "yarn": ">=1.22.19"
  },
  "bugs": {
    "url": "https://github.com/mpnproject/discord-bot-server/issues"
  },
  "homepage": "https://github.com/mpnproject/discord-bot-server#readme",
  "scripts": {
    "start:dev": "concurrently \"tsc -w\" \"nodemon -L build/src/index.js\"",
    "dev": "nodemon -L src/index.ts",
    "start": "tsc && tsc-alias && node -r tsconfig-paths/register dist/index.js",
    "compile": "tsc -p .",
    "compile:dev": "tsc --watch --preserveWatchOutput",
    "heroku-postbuild": "tsc -p .",
    "docs": "node --expose-gc ./node_modules/.bin/apidoc",
    "lint": "eslint --ext .ts .",
    "commit-lint": "npx --no -- commitlint --edit ${1}",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:ci": "yarn test -- --coverage",
    "prepare": "husky install",
    "deploy:commands": "tsc && tsc-alias && node -r tsconfig-paths/register dist/discord/deploy-commands.js"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo ”[Husky] pre-commit”"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.186",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.23",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "apidoc": "^0.53.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "faker": "^6.6.6",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "nock": "^13.2.9",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "discord.js": "^14.5.0",
    "discordx": "^11.4.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "monapt": "^2.1.0",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  }
}
